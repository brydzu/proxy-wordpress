{
    "name": "redirectionio/wordpress-plugin",
    "description": "redirection.io WordPress Plugin",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Gr√©goire Pineau",
            "email": "lyrixx@lyrixx.info"
        },
        {
            "name": "Quentin Brunet",
            "email": "hello@quentinbrunet.com"
        },
        {
            "name": "redirection.io",
            "email": "coucou@redirection.io"
        }
    ],
    "require": {
        "redirectionio/sdk": "dev-master"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4@beta",
        "symfony/process": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "RedirectionIO\\Client\\Wordpress\\": "src/"
        }
    },
    "repositories": [
        {
